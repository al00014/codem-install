#!/bin/bash

export PATH=<%= install_dir %>/bin:<%= install_dir %>/embedded/bin:$PATH
export GEM_PATH=<%= install_dir %>/embedded/lib/ruby/gems/2.1.0
export GEM_HOME=<%= install_dir %>/embedded/lib/ruby/gems/2.1.0

# default to RAILS_ENV=production
if [[ -z $RAILS_ENV ]]; then
  export RAILS_ENV=production
fi

cd <%= install_dir %>/codem-scheduler
<%= install_dir %>/embedded/bin/bundle exec <%= command %>

